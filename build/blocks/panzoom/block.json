{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "occ/rather-simple-panzoom",
  "title": "Rather Simple Panzoom",
  "description": "Display a panzoom image.",
  "textdomain": "rather-simple-panzoom",
  "category": "media",
  "attributes": {
    "id": {
      "type": "integer",
      "default": 0
    },
    "url": {
      "type": "string",
      "default": ""
    },
    "alt": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css",
  "editorStyle": "file:./index.css"
}